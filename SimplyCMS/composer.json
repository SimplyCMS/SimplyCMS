{
    "name": "simplycms/core",
    "type": "library",
    "description": "A Simply Modern CMS [CORE]",
    "keywords": [
        "cms",
        "laravel",
        "simplycms"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "fideloper/proxy": "^4.0",
        "symfony/yaml": "^5.0",
        "twig/twig": "^3.0",

        "illuminate/auth": "^6.8",
        "illuminate/broadcasting": "^6.8",
        "illuminate/bus": "^6.8",
        "illuminate/cache": "^6.8",
        "illuminate/config": "^6.8",
        "illuminate/console": "^6.8",
        "illuminate/container": "^6.8",
        "illuminate/contracts": "^6.8",
        "illuminate/cookie": "^6.8",
        "illuminate/database": "^6.8",
        "illuminate/encryption": "^6.8",
        "illuminate/events": "^6.8",
        "illuminate/filesystem": "^6.8",
        "illuminate/hashing": "^6.8",
        "illuminate/http": "^6.8",
        "illuminate/log": "^6.8",
        "illuminate/mail": "^6.8",
        "illuminate/notifications": "^6.8",
        "illuminate/pagination": "^6.8",
        "illuminate/pipeline": "^6.8",
        "illuminate/queue": "^6.8",
        "illuminate/redis": "^6.8",
        "illuminate/routing": "^6.8",
        "illuminate/session": "^6.8",
        "illuminate/support": "^6.8",
        "illuminate/translation": "^6.8",
        "illuminate/validation": "^6.8"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SimplyCMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimplyCMS\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
